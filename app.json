{
  "expo": {
    "name": "El-Clasico",
    "description": "A simple Score-Keeper App for Football Games",
    "slug": "el-clasico",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.vibhu4agarwal.elClasico",
      "versionCode": 1
    }
  }
}
